<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <listener>
        <description>ServletContextListener, HttpSessionListener</description>
        <listener-class>activeUser</listener-class>
    </listener>
    <listener>
        <description>ServletContextListener</description>
        <listener-class>ContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>validateServlet</servlet-name>
        <servlet-class>backend.validateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>welcomeServlet</servlet-name>
        <servlet-class>frontend.welcomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>invalidationServlet</servlet-name>
        <servlet-class>invalidationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fileuploader</servlet-name>
        <servlet-class>fileuploader</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>alluserlistcreate</servlet-name>
        <servlet-class>alluserlistcreate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addremove</servlet-name>
        <servlet-class>addremove</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>newsremover</servlet-name>
        <servlet-class>newsremover</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>generator</servlet-name>
        <servlet-class>generator</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>verifierplus</servlet-name>
        <servlet-class>verifierplus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileDownloadServlet</servlet-name>
        <servlet-class>FileDownloadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>validateServlet</servlet-name>
        <url-pattern>/validateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>welcomeServlet</servlet-name>
        <url-pattern>/welcomeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>invalidationServlet</servlet-name>
        <url-pattern>/invalidationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fileuploader</servlet-name>
        <url-pattern>/fileuploader</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>alluserlistcreate</servlet-name>
        <url-pattern>/alluserlistcreate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addremove</servlet-name>
        <url-pattern>/addremove</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>newsremover</servlet-name>
        <url-pattern>/newsremover</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>generator</servlet-name>
        <url-pattern>/generator</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>verifierplus</servlet-name>
        <url-pattern>/verifierplus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileDownloadServlet</servlet-name>
        <url-pattern>/FileDownloadServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>50</session-timeout>
    </session-config>
</web-app>
